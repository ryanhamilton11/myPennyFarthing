@model IQueryable<MX><fieldset class="border d-inline-block p-3 shadow">    <legend> Maintenance </legend>    <table>        <thead>            @foreach (MX mx in Model)            {<tr>    <td>        @mx.Date.ToString("MM-dd-yyy")        @mx.Action        @mx.Mileage.ToString("N2")        @mx.Cost.ToString("N2")    </td>    <td>        <a asp-controller="Maintenance" asp-action="Details" asp-route-id="@mx.Id" class="btn btn-sm btn-primary"> Details </a>    </td></tr>}        </thead>        <tbody>            <tr> <td colspan="2"> <hr /> </td> </tr>            <tr>                <td class="text-center" colspan="2">                    <a asp-controller="MX" asp-action="Create" class="btn btn-sm btn-outline-success"> Add Maintenance </a>                </td>            </tr>        </tbody>    </table></fieldset>